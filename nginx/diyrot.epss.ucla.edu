server {
	
	listen 80
    
	client_max_body_size 1000M;

	root /var/www/dpr-dev.epss.ucla.edu;

	server_name diyrot.epss.ucla.edu;

	location /bin/ {
		deny all;
	}

	location /return/ {
	}

	location /upload/ {
		include uwsgi_params;
		uwsgi_pass unix:/tmp/upload.sock;
	}

	location /detect/ {
		include uwsgi_params;
		uwsgi_pass unix:/tmp/upload.sock;
	}

	location /derot/ {
		include uwsgi_params;
		uwsgi_pass unix:/tmp/upload.sock;
	}

	location / {
		index index.html;
	}

    
 

}
