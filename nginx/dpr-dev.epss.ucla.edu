server {
	listen 80 default_server;
	listen [::]:80 default_server;

	root /tmp/www;

	server_name dpr-dev.epss.ucla.edu;

	location /bin/ {
		deny all;
	}

	location /return/ {
	}

	location /upload/ {
		include uwsgi_params;
		uwsgi_pass unix:/tmp/upload.sock;
	}

	location / {
		index index.html;
	}
}
